<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<ScrollPane fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphics.app.StatsFXML">
  <content>
      <VBox fx:id="display" maxHeight="1.7976931348623157E308" spacing="10.0">
         <children>
            <HBox VBox.vgrow="ALWAYS">
               <children>
                  <Label text="Today's total views:" />
                  <Label fx:id="views" text="Label" />
               </children>
            </HBox>
            <HBox>
               <children>
                  <Label text="Today's total likes:" />
                  <Label fx:id="likes" text="Label" />
               </children>
            </HBox>
         </children></VBox>
  </content>
</ScrollPane>
